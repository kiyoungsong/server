@inherits LayoutComponentBase

<Layout>
    <HeaderLayout CollapsedChanged="CollapsedChanged"></HeaderLayout>
    <Layout>
        <Sider Width="200" Collapsed="@collapsed" Class="site-layout-background">
            <SideBar Collapsed="@collapsed" DataXml="@dataInfo"></SideBar>
        </Sider>
        <Layout Style=" padding: 24px 0px 24px 24px;">
            <Content Class="site-layout-background" Style=" padding: 24px; margin: 0; min-height: 800px;">
                @Body
            </Content>
            <FooterLayout></FooterLayout>
        </Layout>
    </Layout>
</Layout>


